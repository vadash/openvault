{
  "outputFilePath": "repomix-output.txt",
  "include": [
    "scripts/**/*",
    "src/**/*",
    "tests/**/*",
    "templates/**/*",
    "example/**/*",
    "*.js",
    "*.json",
    "*.css",
    "*.md"
  ],
  "ignore": {
    "useGitignore": true,
    "customPatterns": [
      "style.css",
      "docs/**",
      "repomix-output.*",
      "tmp/**",
      ".worktrees/**",
      "logs/**",
      "node_modules/**",
      "package-lock.json"
    ]
  }
}
